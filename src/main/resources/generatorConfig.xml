<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="dsql" targetRuntime="MyBatis3DynamicSql">
        <jdbcConnection driverClass="org.sqlite.JDBC"
                        connectionURL="jdbc:sqlite:mortybot.db" />

        <javaModelGenerator targetPackage="net.hatemachine.mortybot.model" targetProject="src/main/java"/>
        <javaClientGenerator targetPackage="net.hatemachine.mortybot.mapper" targetProject="src/main/java"/>

        <table tableName="bot_users" domainObjectName="BotUser" />
    </context>
</generatorConfiguration>
